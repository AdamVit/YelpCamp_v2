<%- include("../partials/header") %> 

<h1 class="m-4">Notifications</h1>

<div class="row">
	<div class="col-md-8 col-lg-4">
		<div class="list-group">
		<% allNotifications.forEach(function(notification) { %>
		<a class="list-group-item list-group-item-action" href="/campgrounds/<%= notification.campgroundId %>">
			<%= notification.username %> created a new campground %>
		</a>
		<% }); %>
		</div>
	</div>	
</div>

<%- include("../partials/footer") %> 